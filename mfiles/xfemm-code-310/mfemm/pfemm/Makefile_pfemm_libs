	$(MAKE) -C libfemm clean -f libfemm.cbp.mak

clean_libfproc_libfproc:
	$(MAKE) -C fpproc clean -f libfpproc.cbp.mak

clean_libfsolver_libfsolver:
	$(MAKE) -C fsolver clean -f libfsolver.cbp.mak

clean_libfmesher_libfmesher: clean_liblua_liblua
	$(MAKE) -C fmesher clean -f libfmesher.cbp.mak

clean_liblua_liblua: clean_libfemm_libfemm
