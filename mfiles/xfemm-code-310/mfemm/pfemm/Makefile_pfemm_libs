WRKDIR = $(CURDIR)
MAKE = make

all: libfmesher_libfmesher libfsolver_libfsolver libfproc_libfproc   

libfemm_libfemm: 
	$(MAKE) -C libfemm all -f libfemm.cbp.mak

libfproc_libfproc:
	$(MAKE) -C fpproc all -f libfpproc.cbp.mak

libfsolver_libfsolver:
	$(MAKE) -C fsolver all -f libfsolver.cbp.mak

